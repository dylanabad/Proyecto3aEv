<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="230.0" prefWidth="400" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="controller.LoginController">
    <children>
        <Label layoutX="45.0" layoutY="29.0" text="Email:" />
        <TextField fx:id="emailField" layoutX="120" layoutY="25" prefWidth="250" />

        <Label layoutX="30" layoutY="80" text="Contraseña:" />
        <PasswordField fx:id="passwordField" layoutX="120.0" layoutY="76.0" prefWidth="250" />

        <Button layoutX="125.0" layoutY="145.0" onAction="#handleLogin" prefWidth="150" text="Iniciar sesión" />
    </children>
</AnchorPane>
